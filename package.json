{
  "name": "dsc-lab",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "migrate": "prisma db push",
    "mode:dev": "cross-env MODE=dev nest start --watch",
    "mode:production": "cross-env MODE=production nest start",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test:unit": "jest --config ./test/jest.unit.json",
    "test:unit:watch": "jest --config ./test/jest.unit.json --watch",
    "test:unit:cov": "jest --config ./test/jest.unit.json --coverage",
    "test:e2e": "cross-env MODE=test jest --config ./test/jest-e2e.json",
    "dev": "nest start --watch",
    "db:dev:up": "cd db-docker && docker-compose up -d",
    "db:dev:down": "cd db-docker && docker-compose down"
  },
  "dependencies": {
    "@golevelup/nestjs-rabbitmq": "^4.0.0",
    "@hoplin/nestjs-logger": "^1.0.7",
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^3.0.0",
    "@nestjs/core": "^10.2.0",
    "@nestjs/jwt": "^10.1.1",
    "@nestjs/microservices": "^10.1.3",
    "@nestjs/passport": "^10.0.1",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/swagger": "^7.1.8",
    "@nestjs/typeorm": "^10.0.0",
    "@prisma/client": "^5.2.0",
    "@slack/webhook": "^6.1.0",
    "@yuzu441/is-ulid": "^1.1.0",
    "amqp-connection-manager": "^4.1.14",
    "amqplib": "^0.10.3",
    "bcryptjs": "^2.4.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "discord.js": "^14.13.0",
    "dotenv": "^16.3.1",
    "luxon": "^3.4.3",
    "mysql2": "^3.6.0",
    "nodemailer": "^6.9.4",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "prisma": "^5.2.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1",
    "ulid": "^2.3.0",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@slack/types": "^2.8.0",
    "@types/amqplib": "^0.10.1",
    "@types/bcryptjs": "^2.4.4",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.2",
    "@types/luxon": "^3.3.2",
    "@types/node": "^20.3.1",
    "@types/nodemailer": "^6.4.9",
    "@types/passport-jwt": "^3.0.9",
    "@types/supertest": "^2.0.12",
    "@types/uuid": "^9.0.2",
    "@typescript-eslint/eslint-plugin": "^5.59.11",
    "@typescript-eslint/parser": "^5.59.11",
    "cross-env": "^7.0.3",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.5.0",
    "prettier": "^2.8.8",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "moduleNameMapper": {
      "test/(.*)$": "<rootDir>/../test/$1",
      "src/(.*)$": "<rootDir>/$1",
      "^@app/(.*)$": "<rootDir>/app/$1",
      "^@infrastructure/(.*)$": "<rootDir>/infrastructure/$1",
      "^@domain(.*)$/": "<rootDir>/domain/$1"
    }
  }
}
